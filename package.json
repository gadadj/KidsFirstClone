{
  "name": "kids-first-portal-ui",
  "version": "5.5.0",
  "private": true,
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run lint&fix"
    ],
    "!src/cypress/**/*.{ts,tsx}": "echo Ignoring Cypress files"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "start": "DISABLE_ESLINT_PLUGIN=true node scripts/start.js",
    "build": "DISABLE_ESLINT_PLUGIN=true node scripts/build.js",
    "build:netlify": "npm ci && npm run build",
    "test": "node scripts/test.js --coverage --coverageReporters=\"json-summary\"",
    "test:github": "node scripts/test.js --coverage --watchAll=false --no-watchman --coverageReporters=\"json-summary\"",
    "coverage": "npm test -- --watchAll=false --coverage --coverageReporters=\"json-summary\"",
    "coverage:changed": "npm test -- --coverage --watchAll=false --onlyChanged --coverageReporters=\"json-summary\"",
    "lint": "eslint $(git diff-index --name-only --diff-filter=d HEAD | grep \"[\\.js|\\.jsx|\\.tsx|\\.ts]$\" | xargs)",
    "lint:fix": "eslint --fix --rule 'no-console: 2'",
    "theme": "mkdir -p src/style/themes/kids-first/dist && npm run theme:antd && npm run theme:colors",
    "theme:watch": "nodemon --watch './src/style/themes' -e less --exec 'npm run theme'",
    "theme:antd": "npm run theme:antd:main",
    "theme:antd:main": "lessc --js src/style/themes/kids-first/antd-kids-first-theme.less src/style/themes/kids-first/dist/antd.css",
    "theme:colors": "ferlabui-less2css src/style/themes/kids-first/ colors.less",
    "prepare": "npm run theme",
    "storybook": "DISABLE_ESLINT_PLUGIN=true start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "cypress:install": "cypress install",
    "cypress:open": "cypress open",
    "cypress:run:cli": "cypress run",
    "cypress:run:chrome": "cypress run -b chrome"
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "all",
    "singleQuote": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kids-first/kf-portal-ui.git"
  },
