    "prepare": "npm run theme",
    "pretest": "lint-staged",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "test:unit": "cross-env NODE_ENV=\"test\" NODE_PATH=\"src\" nyc --reporter=html mocha --watch --require ./node_modules/@babel/register src/**/*.unit.js",
